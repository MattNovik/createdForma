{
  "name": "stud",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mui/material": "^5.10.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "craco": "^0.0.3",
    "formik": "^2.2.9",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-imask": "^6.4.3-alpha.0",
    "react-scripts": "5.0.1",
    "sass": "^1.54.9",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "craco start",
    "build:app": "react-scripts build",
    "build:bundle": "webpack --mode production",
    "build-rename": "npm run build-rename-js && npm run build-rename-css",
    "build-rename-js": "renamer --regex --find 'main\\.[^\\.]+\\.js' --replace 'main.js' build/static/js/*.js",
    "build-rename-css": "renamer --regex --find 'main\\.[^\\.]+\\.css' --replace 'main.css' build/static/css/*.css",
    "build": "npm run build:app && npm run build-rename",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.5",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.3",
    "cssnano": "^4.1.11",
    "file-loader": "^6.2.0",
    "parcel-bundler": "^1.12.4",
    "path": "^0.12.7",
    "renamer": "^4.0.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.6",
    "webpack-cli": "^5.0.1"
  }
}
